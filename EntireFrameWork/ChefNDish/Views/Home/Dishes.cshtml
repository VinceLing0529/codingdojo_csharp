@model Chef
<div class="text-center">
<h1> <a asp-action="Index">Chefs</a>|Dishes</h1>
</div>
<h2><a asp-action="NewDish">AddADish</a></h2>
<h2>Yum, take a look at our tasty dishes!</h2>

<table class="table table-striped">
    <tr>
                        <th>Name</th>
                        <th>Chef</th>
                        <th>Tastiness</th>
                        <th>Calories</th>

                    </tr>

    @foreach (var i in ViewBag.AllDish)
    {
        <tr>
            <td>@i.Name</td>
            <td>@i.creater.FirstName @i.creater.LastName</td>
            <td>@i.Tastiness</td>
            <td>@i.Calories</td>
        </tr>
    }
</table>